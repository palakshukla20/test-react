{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "ignorePatterns":  ["*.test.js", "*.json" ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
       "react/prop-types": "off",
       "indent": ["error", 2],
       "object-curly-spacing": ["error", "never"]
    }
}
